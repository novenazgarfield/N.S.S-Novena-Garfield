{
  "version": 2,
  "name": "nexus-research-workstation",
  "builds": [
    {
      "src": "landing_page/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/landing_page/index.html"
    },
    {
      "src": "/landing_page/(.*)",
      "dest": "/landing_page/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}